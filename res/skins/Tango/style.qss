WBeatSpinBox {
  /* make it 2px smaller in each dimension,
    border is added for actual size */
  min-width: 52px;
  max-width: 52px;
  min-height: 20px;
  max-height: 20px;
  border: 1px solid #666666; /* = 54x22px */
  border-radius: 2px;
  background-color: #0f0f0f;
  color: #aaaaaa;
  selection-background-color: #d2d2d2;
  selection-color: #0f0f0f;
  font-size: 13px/13px;
  padding: 0;
}
  WBeatSpinBox:hover {
    border-color: #888888;
  }
  WBeatSpinBox:focus {
    border-color: #d2d2d2;
    color: #cccccc;
  }

  WBeatSpinBox::up-button, WBeatSpinBox::down-button {
    subcontrol-origin: margin;
    position: relative;
    /* as with spinbox: border is added to size.
      define it with same width as hover border, otherwise size jumps */
    width: 18px;
    height: 18px;
    border: 0px solid #777777; /* = 18x18px */
    border-radius: 8px;
    background-color: #333333;
    }

  WBeatSpinBox::up-button {
    image: url(skin:/buttons/btn_arrow_up.svg);
    }
    WBeatSpinBox::up-button:hover {
      image: url(skin:/buttons/btn_arrow_up_hover.svg);
    }
  WBeatSpinBox::down-button {
    image: url(skin:/buttons/btn_arrow_down.svg);
    }
    WBeatSpinBox::down-button:hover {
      image: url(skin:/buttons/btn_arrow_down_hover.svg);
    }

  #LoopBeatJumpContainer_Right > WBeatSpinBox {
    padding-left: 3px;
    margin: 0 19px 0 0;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    }
    #LoopBeatJumpContainer_Right > WBeatSpinBox::up-button {
      subcontrol-position: center right;
      }
    #LoopBeatJumpContainer_Right > WBeatSpinBox::down-button {
      subcontrol-position: center right;
      left: -13px;
    }

  #LoopBeatJumpContainer_Left > WBeatSpinBox {
    margin: 0 0 0 19px;
    border-top-left-radius: 8px;
    border-bottom-left-radius: 8px;
    }
    #LoopBeatJumpContainer_Left > WBeatSpinBox::up-button {
      subcontrol-position: center left;
      right: -13px;
      }
    #LoopBeatJumpContainer_Left > WBeatSpinBox::down-button {
      subcontrol-position: center left;
    }

